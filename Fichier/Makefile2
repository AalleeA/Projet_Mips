SRC_PATH = src/
BIN_PATH = bin/


Mips : $(BIN_PATH)MIPS.o $(BIN_PATH)Read.o $(BIN_PATH)ecritureFichier.o $(BIN_PATH)Registre.o $(BIN_PATH)charToHexa.o
	gcc -o $@ -Wall -lm $^
	clear
	./Mips Test/Test5.txt

$(BIN_PATH)MIPS.o : $(SRC_PATH)MIPS.c
	gcc -o $@ -Wall $<

$(BIN_PATH)Read.o : $(SRC_PATH)Read.c
	gcc -o $@ -Wall $<

$(BIN_PATH)ecritureFichier.o : $(SRC_PATH)ecritureFichier.c
	gcc -o $@ -Wall $<

$(BIN_PATH)Registre.o : $(SRC_PATH)Registre.c
	gcc -o $@ -Wall $<

$(BIN_PATH)charToHexa.o : $(SRC_PATH)charToHexa.c
	gcc -o $@ -Wall $< -lm
